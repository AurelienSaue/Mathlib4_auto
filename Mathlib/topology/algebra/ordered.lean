theorem is_open_lt {Œ± : Type u} {Œ≤ : Type v} [topological_space Œ±] [linear_order Œ±] [order_closed_topology Œ±] [topological_space Œ≤] {f : Œ≤ ‚Üí Œ±} {g : Œ≤ ‚Üí Œ±} (hf : continuous f) (hg : continuous g) : is_open (set_of fun (b : Œ≤) => f b < g b) := sorry

