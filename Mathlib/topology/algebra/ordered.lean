/-- The order topology on an ordered type is the topology generated by open intervals. We register
it on a preorder, but it is mostly interesting in linear orders, where it is also order-closed.
We define it as a mixin. If you want to introduce the order topology on a preorder, use
`preorder.topology`. -/
class order_topology (Œ± : Type u_1) [t : topological_space Œ±] [preorder Œ±] 
where
  topology_eq_generate_intervals : t = topological_space.generate_from (set_of fun (s : set Œ±) => ‚àÉ (a : Œ±), s = set.Ioi a ‚à® s = set.Iio a)

/-- (Order) topology on a partial order `Œ±` generated by the subbase of open intervals
`(a, ‚àû) = { x ‚à£ a < x }, (-‚àû , b) = {x ‚à£ x < b}` for all `a, b` in `Œ±`. We do not register it as an
instance as many ordered sets are already endowed with the same topology, most often in a non-defeq
way though. Register as a local instance when necessary. -/
def preorder.topology (Œ± : Type u_1) [preorder Œ±] : topological_space Œ± :=
  topological_space.generate_from
    (set_of fun (s : set Œ±) => ‚àÉ (a : Œ±), (s = set_of fun (b : Œ±) => a < b) ‚à® s = set_of fun (b : Œ±) => b < a)

protected instance order_dual.order_topology {Œ± : Type u_1} [topological_space Œ±] [partial_order Œ±] [order_topology Œ±] : order_topology (order_dual Œ±) := sorry

theorem is_open_iff_generate_intervals {Œ± : Type u} [topological_space Œ±] [partial_order Œ±] [t : order_topology Œ±] {s : set Œ±} : is_open s ‚Üî topological_space.generate_open (set_of fun (s : set Œ±) => ‚àÉ (a : Œ±), s = set.Ioi a ‚à® s = set.Iio a) s := sorry

theorem is_open_lt' {Œ± : Type u} [topological_space Œ±] [partial_order Œ±] [t : order_topology Œ±] (a : Œ±) : is_open (set_of fun (b : Œ±) => a < b) :=
  eq.mpr (id (Eq._oldrec (Eq.refl (is_open (set_of fun (b : Œ±) => a < b))) (propext is_open_iff_generate_intervals)))
    (topological_space.generate_open.basic (set_of fun (b : Œ±) => a < b) (Exists.intro a (Or.inl rfl)))

theorem is_open_gt' {Œ± : Type u} [topological_space Œ±] [partial_order Œ±] [t : order_topology Œ±] (a : Œ±) : is_open (set_of fun (b : Œ±) => b < a) :=
  eq.mpr (id (Eq._oldrec (Eq.refl (is_open (set_of fun (b : Œ±) => b < a))) (propext is_open_iff_generate_intervals)))
    (topological_space.generate_open.basic (set_of fun (b : Œ±) => b < a) (Exists.intro a (Or.inr rfl)))

theorem lt_mem_nhds {Œ± : Type u} [topological_space Œ±] [partial_order Œ±] [t : order_topology Œ±] {a : Œ±} {b : Œ±} (h : a < b) : filter.eventually (fun (x : Œ±) => a < x) (nhds b) :=
  mem_nhds_sets (is_open_lt' a) h

theorem le_mem_nhds {Œ± : Type u} [topological_space Œ±] [partial_order Œ±] [t : order_topology Œ±] {a : Œ±} {b : Œ±} (h : a < b) : filter.eventually (fun (x : Œ±) => a ‚â§ x) (nhds b) :=
  filter.sets_of_superset (nhds b) (lt_mem_nhds h)
    fun (b : Œ±) (hb : b ‚àà set_of fun (x : Œ±) => (fun (x : Œ±) => a < x) x) => le_of_lt hb

