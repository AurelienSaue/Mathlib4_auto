@[simp] theorem nhds_within_Ioc_eq_nhds_within_Ioi {Œ± : Type u} [topological_space Œ±] [linear_order Œ±] [order_closed_topology Œ±] {a : Œ±} {b : Œ±} (h : a < b) : nhds_within a (set.Ioc a b) = nhds_within a (set.Ioi a) :=
  le_antisymm (nhds_within_mono a set.Ioc_subset_Ioi_self)
    (nhds_within_le_of_mem (Ioc_mem_nhds_within_Ioi (iff.mpr set.left_mem_Ico h)))

